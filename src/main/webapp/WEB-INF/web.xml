<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>CDR Web Application</display-name>
  <servlet>
    <servlet-name>ner</servlet-name>
    <servlet-class>
      edu.ktlab.bionlp.cdr.web.NERServlet
    </servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ner</servlet-name>
    <url-pattern>/ner</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>cid</servlet-name>
    <servlet-class>
      edu.ktlab.bionlp.cdr.web.CIDServlet
    </servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>cid</servlet-name>
    <url-pattern>/cid</url-pattern>
  </servlet-mapping>
</web-app>
